{
  "name": "@trungung/wt",
  "version": "0.1.0",
  "type": "module",
  "description": "A CLI built with Bunli",
  "main": "./dist/index.js",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "author": "",
  "bin": {
    "wt": "dist/index.js"
  },
  "scripts": {
    "postinstall": "bunli generate",
    "dev": "bun run src/index.ts",
    "build": "bunli build && bun build src/index.ts --outfile dist/index.js --target bun --format esm --minify",
    "test": "bun test",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@bunli/core": "latest",
    "glob": "^13.0.0",
    "prompts": "^2.4.2"
  },
  "devDependencies": {
    "@bunli/test": "latest",
    "@types/bun": "latest",
    "@types/prompts": "^2.4.9",
    "bunli": "latest",
    "typescript": "^5.0.0"
  },
  "bunli": {
    "entry": "./src/index.ts",
    "outDir": "./dist"
  }
}
